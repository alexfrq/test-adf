{
	"name": "Datalake_parquet_snappy",
	"properties": {
		"linkedServiceName": {
			"referenceName": "AzureDataLakeStorage1",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"FolderPrefix": {
				"type": "string"
			},
			"TableName": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "Parquet",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@concat(dataset().TableName, '.snappy.parquet')",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@concat(dataset().FolderPrefix, '/', dataset().TableName)",
					"type": "Expression"
				},
				"fileSystem": "landing"
			},
			"compressionCodec": "snappy"
		},
		"schema": []
	}
}